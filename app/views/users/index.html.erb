<h1>Users</h1>
<%= will_paginate %>
<ul>
  <% @users.each do |user| %>
    <li>
      <strong><%= user.first_name %></strong>
      <% unless current_user == user %>
        <%= link_to 'Send message', new_message_path(to: user.id), class: 'btn btn-default btn-sm' %>
      <% end %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
